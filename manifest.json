{
    "manifest_version": 3,
    "name": "No Cookie Banners",
    "description": "This extension automatically clicks cookie banners away.",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "cookie-icon-16.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "utility.js",
                "texts.js",
                "main.js"
            ]
        }
    ]
}